{
  // todo: Aono and Kunii [2]
  "reference": "Prusinkiewicz, Przemyslaw et al. “L-systems: from the Theory to Visual Models of Plants.” (2001).",
  "lSystem": {
    "alphabet": ["A(l,w)", "B(l,w)", "C(l,w)", "F(l,w)", "[", "]", "+(a)", "/(d)", "$"],
    "axiom": "A(1,10)",
    "productions": [
      "A(l,w) -> F(l,w)[&(a1)B(l*r1,w*wr)]/(180.0)[&(a2)B(l*r2,w*wr)]",
      "B(l,w) -> F(l,w)[+(a1)$B(l*r1,w*wr)][-(a2)$B(l*r2,w*wr)]"
    ],
    "parameters": ["r1", "r2", "a1", "a2", "wr"],
    "parameterPresets": [
      {"r1": 0.9, "r2": 0.7, "a1":  5.0, "a2": 65.0, "wr": 0.707},
      {"r1": 0.9, "r2": 0.7, "a1": 10.0, "a2": 60.0, "wr": 0.707},
      {"r1": 0.9, "r2": 0.8, "a1": 20.0, "a2": 50.0, "wr": 0.707},
      {"r1": 0.9, "r2": 0.8, "a1": 35.0, "a2": 35.0, "wr": 0.707}
    ],
    "defaultPreset": 0,
    "maxIterations": 10
  },
  "lightSources": [
    { "color": [0.0, 0.0, 1.0], "position": [1.0, 0.0, 0.0] },
    { "color": [1.0, 0.0, 0.0], "position": [-1.0, 0.0, 0.0] }
  ],
  "materials": [
    { "color":  [1.0, 1.0, 1.0] }
  ]
}
